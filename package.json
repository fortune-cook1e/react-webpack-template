{
  "name": "react-webpack-template",
  "version": "1.0.0",
  "repository": "https://github.com/fortune-cook1e/react-webpack-template.git",
  "author": "fortune cookie <767077147@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "start": "cross-env vite",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "analyze": "cross-env NODE_ENV=production ANALYSIS=1 webpack --config config/webpack.prod.js",
    "lint": "cross-env eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "stylelint": "cross-env stylelint src/**/*.{css,less} --fix",
    "commit": "cz",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "pnpm lint",
      "git add ."
    ],
    "src/**/*.{less,css}": [
      "pnpm stylelint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.6.2",
    "antd": "^4.24.0",
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.4.3",
    "redux": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@types/react-router-config": "^5.0.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^8.0.1",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "lint-staged": "^11.0.1",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.3.0",
    "react-refresh": "^0.10.0",
    "redux-logger": "^3.0.6",
    "style-loader": "^2.0.0",
    "stylelint": "^14.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.3.2",
    "vite": "^3.2.2",
    "webpack": "^5.61.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.7.3"
  }
}
